      license-feature="org.eclipse.license"
      license-feature-version="0.0.0">

   <description>
      Support for UML2 derived features.
   </description>

   <url>
      <update label="EMF-IncQuery" url="http://download.eclipse.org/incquery/updates/release/"/>
   </url>

   <requires>
      <import feature="org.eclipse.uml2.uml" version="5.0.0" match="compatible"/>
      <import feature="org.eclipse.viatra.query.runtime.feature"/>
      <import feature="org.eclipse.xtext.xbase.lib" version="2.9.0" match="compatible"/>
   </requires>

   <plugin
         id="org.eclipse.viatra.integration.uml"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
